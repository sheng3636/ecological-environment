      this.barLineChart('sideItem1_10', this.chartArr.sideItem1_10)
      this.barLineChart('sideItem1_11', this.chartArr.sideItem1_11)
      
      this.horizontalBarLineChart('sideItem2_0', this.chartArr.sideItem2_0)
      this.barLineChart('sideItem2_2', this.chartArr.sideItem2_2)
      this.barLineChart('sideItem2_3', this.chartArr.sideItem2_3)
      this.barLineChart('sideItem2_4', this.chartArr.sideItem2_4)
      this.horizontalBarLineChart('sideItem2_5', this.chartArr.sideItem2_5)

      this.barLineChart('sideItem3_0', this.chartArr.sideItem3_0)
      this.barLineChart('sideItem3_1', this.chartArr.sideItem3_1)
      this.barLineChart('sideItem3_2', this.chartArr.sideItem3_2)
