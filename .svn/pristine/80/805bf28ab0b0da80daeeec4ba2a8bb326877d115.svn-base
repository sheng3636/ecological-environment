    },
    // 迁移图弹窗列表切换
    migrationTypeClick(val) {
      this.migrationType = val
      this.flowMapTableListFn(val)
    },
    // 筛选迁移地图弹窗右侧列表数据
    flowMapTableListFn(flag) {
      this.flowMapTableList = []
      let listDirt = {}
      let listDirt1 = {}
      this.aaa.forEach(d => {
        let name = d.leaveName
        let name1 = d.arriveName
        if (!listDirt[name]) {
          listDirt[name] = 0
        }
        if (!listDirt1[name1]) {
          listDirt1[name1] = 0
        }
        listDirt[name] += parseFloat(d.tripNumber)
        listDirt1[name1] += parseFloat(d.tripNumber)
      })
      let listObj = null
      if (flag === 0) {
        listObj = listDirt1
      } else {
        listObj = listDirt
      }

      for (const key in listObj) {
        this.flowMapTableList.push({
          name: key,
          value: listObj[key]
        })
      }
      this.flowMapTableList.sort((a, b) => {
        return b.value - a.value
      })
    },
    // 打开迁移图弹窗
    flowMapDialogOpen(key) {
      this.flowMapVisi = true
      this.whichMap = key
      let data = this.chartArr[key].flowMapList
      this.flowMapDiaTitle = this.chartArr[key].title
      this.aaa = data
      this.flowMapTableListFn(0)
      let bbb = data.filter(item => {
        return (item.leaveName === this.chartArr[key].configObj.dialogCity) || (item.arriveName === this.chartArr[key].configObj.dialogCity)
      })
      this.$nextTick(() => {
        this.createContactMap(
          'mapArea',
          bbb,
          this.chartArr[key].configObj.curCity
        )
      })
    },
    // 关闭迁移图弹窗
    flowMapDialogClose() {
      this.flowMapVisi = false
      this.migrationType = 0
      this.whichMap = ''
    },
    // 下载文档
    downloadDoc() {
      let _self = this
      _self.initChart('#000')
      _self.fullscreenLoading = true
      let baseURlArr = []
      let infoArr = []
      let picArrIndex = 0
      let picArrFn = []

      setTimeout(function() {
        for (let key in _self.chartArr) {
          if (!_self.chartArr[key].picHidden) {
            let chart = _self.$echarts.init(
              document.getElementById(_self.chartArr[key].id)
            )
            baseURlArr[key] = chart.getDataURL()
            picArrFn[picArrIndex] = savePic({
              picInfo: baseURlArr[key],
              no: 'no' + _self.chartArr[key].picNum,
              type: 'people-county',
              area: _self.cityName
            }).then(res => {})
            picArrIndex++
          }
          infoArr.push(
            {
              name: 'title' + _self.chartArr[key].titleNum,
              value: _self.chartArr[key].title
            },
            {
              name: 'verdict' + _self.chartArr[key].infoNum,
              value: _self.chartArr[key].result
            },
            {
              name: 'source' + _self.chartArr[key].titleNum,
              value: _self.chartArr[key].dataSource
            },
            {
              name: 'table' + _self.chartArr[key].tableNum,
              value: _self.chartArr[key].list
            }
          )
        }
        let aa = infoArr.filter(item => {
          return item.value
        })

        axiosAll(picArrFn).then(res => {
          axios({
            url: _self.baseUrl + '/section/exportWord',
            method: 'post',
            data: {
              area: _self.cityName,
              type: 'people-county',
              condition: JSON.stringify(aa)
            },
            responseType: 'blob',
            onDownloadProgress(progress) {
              _self.filePercentage = Math.round(
                (progress.loaded / progress.total) * 100
              )
            }
          }).then(res => {
            // new Blob([data])用来创建URL的file对象或者blob对象
            let url = window.URL.createObjectURL(new Blob([res.data]))
            let link = document.createElement('a')
            link.style.display = 'none'
            link.href = url
            let fileName = `${_self.countyName}${
              _self.$route.meta.moudleSystem
            }报告${_self.returnDatetime()}` // 生成一个a标签
            link.download = fileName + '.docx'
            document.body.appendChild(link)
            setTimeout(function() {
              link.click()
              _self.initChart('#fff')
              _self.filePercentage = 0
              _self.fullscreenLoading = false
            }, 1000)
          })
        })
      }, 1500)
    }
  }
}
</script>
