@import '@/assets/styles/mixin.scss';
.waterTypeTableWrap,
.rankTable {
  padding: 5px;
  @include sc(20px, #fff);

  li {
    display: flex;
    font-size: 20px;
    text-align: center;

    p {
      flex: 1;
      line-height: 45px;

      &.yellow {
        color: #ffba00;
      }
    }

    &:nth-child(odd) {
      background: #0b274a;
    }
  }
}

.rankTable {
  margin-top: 10px;
  border: 2px solid #007e7d;
}