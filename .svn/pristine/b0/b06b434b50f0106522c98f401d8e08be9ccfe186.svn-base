@import './variables.scss';
@import './mixin.scss';

html,body {
  height: 100%;
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  font-family: Helvetica Neue,
  Helvetica,
  PingFang SC,
  Hiragino Sans GB,
  Microsoft YaHei,
  Arial,
  sans-serif;
  color: #fff;
  background: #050b24;
  overflow-x: hidden;
  box-sizing: border-box;
  @include scrollBar()
}
* {
  padding: 0;
  margin: 0;
  list-style: none
}

#app {
  height: 100%;
  color: #fff;
}

a:focus,
a:active {
  outline: none;
}

a,
a:focus,
a:hover {
  cursor: pointer;
  color: inherit;
  text-decoration: none;
}

// 顶部导航栏
.navTopArea {
  height: 100px;
  @include fj(flex-start);

  .navLeft {
    align-items: center;
    width: 600px;
    background: url('../../assets/images/nav/logoBg.png') no-repeat left bottom;
    @include fj(flex-start);
    @include sc(36px, #02cfe4);

    img {
      margin-right: 10px;
    }

    span {
      font-size: 36px;
      font-weight: bold;
      background: linear-gradient(to top, #01a5e8, #00e0d2);
      -webkit-background-clip: text;
      color: transparent;
      letter-spacing: 3px;
    }
  }

  .navRight {
    align-items: center;
    width: calc(100% - 660px);
    border-bottom: 1px solid #004c91;
    @include fj(flex-start);

    li {
      min-width: 120px;
      line-height: 100px;
      cursor: pointer;

      img {
        vertical-align: middle;
      }

      span {
        margin-left: 10px;
        vertical-align: middle;
        @include sc(22px, #09b7ff);
      }

      &.navActive {
        background: url('../../assets/images/navActiveBg.png') no-repeat center bottom;
      }
    }
  }
}

// 底部导航栏
.navBottomArea {
  align-items: center;
  margin: 60px -38px 0 -38px;
  height: 235px;
  border-top: 2px solid #448fe4;
  @include fj(center);
  li {
    position: relative;
    flex-direction: column;
    align-items: center;
    width: 330px;
    cursor: pointer;
    @include sc(24px, #696969);
    @include fj(center);
    img {
      margin-bottom: 15px;
      @include wh(68px,68px);
    }
    span {
      text-align: c;
    }
    .activeImg {
      position: absolute;
      top: -84px;
      @include wh(165px, 52px);
    }
    &.active {
      color: #09b7ff;
    }
  }
}

// 标题分割线
.CuttingLine {
  width: 100%;
  .txtWrap {
    margin: 0 auto;
    @include wh(1620px, 100px);
    align-items: center;
    @include fj();

    .txt {
      font-size: 28px;
      font-weight: bold;
    }
  }
}

// 区域模块标题
.moudleTitle {
  position: relative;
  padding: 0 25px;
  height: 60px;
  align-items: center;
  text-align: left;
  background: linear-gradient(to right, rgba(0, 237, 255, 0) 30%, rgba(0, 237, 255, 0.4) 80%, rgba(0, 237, 255, 0.6) 90%, rgba(0, 237, 255, 0.9));
  @include fj();

  &::before {
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    content: '';
    display: block;
    width: 4px;
    height: 40px;
    background: #00edff;
  }

  .left {
    font-weight: normal;
    @include sc(20px, #00edff);
  }

  .optionGroup {
    width: 80px;
    @include fj();

    .optionImg {
      @include wh(30px, 30px);
      cursor: pointer;
    }
  }
}
.chartIcon {
  margin-right: 5px;
  vertical-align: middle;
  @include wh(24px, 24px);
}
.dataSourceTxt {
  margin: 0 auto 15px auto;
  font-size: 20px;
  font-weight: normal;
  text-align: center;
}

// 结论文字区
.resultWrap {
  margin: 5px 15px 20px 15px;

  h5 {
    margin-bottom: 15px;
    font-size: 16px;
    font-weight: normal;
  }

  .content {
    position: relative;
    align-items: center;
    padding: 10px 70px;
    min-height: 165px;
    background: #0b274a;
    @include fj(center);
    p {
      line-height: 32px;
      text-align: left;
      text-indent: 2em;
      @include sc(16px, #00d0e0);

      .light {
        color:#e09e00;
      }
    }

    .fourIcon {
      position: absolute;
      display: block;
      @include wh(18px, 17px);

      &.fourIcon0 {
        top: 5px;
        left: 5px;
        @include bis('../../assets/images/leftTopIcon.png');
      }

      &.fourIcon1 {
        top: 5px;
        right: 5px;
        @include bis('../../assets/images/rightTopIcon.png');
      }

      &.fourIcon2 {
        bottom: 5px;
        left: 5px;
        @include bis('../../assets/images/leftBottomIcon.png');
      }

      &.fourIcon3 {
        bottom: 5px;
        right: 5px;
        @include bis('../../assets/images/rightBottomIcon.png');
      }
    }
  }
}
.rightResultWrap {
  position: relative;
  align-items: center;
  margin: 10px 10px 0 0;
  padding: 30px;
  width: 470px;
  height: 770px;
  background: #0b274a;
  @include fj(center);
  @include sc(20px, #00d0e0);

  .fourIcon {
    position: absolute;
    display: block;
    @include wh(19px, 17px);

    &.fourIcon0 {
      top: 5px;
      left: 5px;
      @include bis('../../assets/images/leftTopIcon.png');
    }

    &.fourIcon1 {
      top: 5px;
      right: 5px;
      @include bis('../../assets/images/rightTopIcon.png');
    }

    &.fourIcon2 {
      bottom: 5px;
      left: 5px;
      @include bis('../../assets/images/leftBottomIcon.png');
    }

    &.fourIcon3 {
      bottom: 5px;
      right: 5px;
      @include bis('../../assets/images/rightBottomIcon.png');
    }
  }

  .light {
    color: #e09e00;
  }
}

// 下载echart图片弹窗
.imagesWrap {
  position: relative;
  border: 1px solid #00edff;
  background: rgba(5,19,37,0.85)!important;
  
  .fourIcon {
    position: absolute;
    display: block;

    &.fourIcon0 {
      top: -70px;
      left: -15px;
      @include wh(22px, 25px);
      @include bis('../../assets/images/leftTopIcon1.png');
    }

    &.fourIcon1 {
      top: -70px;
      right: -15px;
      @include wh(43px, 47px);
      @include bis('../../assets/images/rightTopIcon1.png');
    }

    &.fourIcon2 {
      bottom: -90px;
      left: -15px;
      @include wh(43px, 47px);
      @include bis('../../assets/images/leftBottomIcon1.png');
    }

    &.fourIcon3 {
      bottom: -90px;
      right: -15px;
      @include wh(22px, 25px);
      @include bis('../../assets/images/rightBottomIcon1.png');
    }
  }

  .el-dialog__headerbtn {
    top: 45px!important;
    right: 55px!important;
  }

  .el-dialog__title {
    line-height: 24px!important;
    font-size: 28px!important;
    color: #00d2ff!important;
  }

  .el-dialog__headerbtn .el-dialog__close {
    color: #6cd6ff !important;
    font-size: 30px !important;
  }
}

// 反馈意见弹窗
.feedbackWrap {
  position: relative;
  width: 1019px;
  height: 580px;
  background: url(../../assets/images/feedbackBg.png) no-repeat center;

  .el-dialog__headerbtn {
    top: 35px !important;
    right: 75px !important;
  }

  .el-dialog__header {
    line-height: 95px;
    background: url(../../assets/images/feedbackHeadeBg.png) no-repeat center;
  }
  .el-dialog__title {
    line-height: 24px !important;
    font-size: 24px !important;
    color: #fff !important;
  }
  .el-dialog__headerbtn .el-dialog__close {
    color: #6cd6ff !important;
    font-size: 50px !important;
    font-weight: bold;
  }
  .el-dialog__body {
    padding: 25px 25px 0px !important;
  }
  .feedbackTitle {
    margin-bottom: 15px;
    font-size: 28px;
    color: #00e3fe;
    text-align: center;
  }

  .feedForm {
    margin: 0 auto;
    width: 95%;

    .el-form-item__label {
      font-size: 16px;
      color: #fff;
    }
  }
}

// 地图意见弹窗
.flowMapWrap {
  position: relative;
  width: 1400px;
  height: 750px;
  background: url(../../assets/images/feedbackBg.png) no-repeat center;
  background-size: 1400px 750px;
  .el-dialog__title {
    line-height: 24px !important;
    font-size: 24px !important;
    color: #fff !important;
  }
  .el-dialog__headerbtn {
    top: 20px !important;
    right: 55px !important;
  }

  .el-dialog__headerbtn .el-dialog__close {
    color: #6cd6ff !important;
    font-size: 50px !important;
    font-weight: bold;
  }

  .el-dialog__body {
    padding: 25px 25px 0px !important;
  }
}
// echarts图类型下拉框样式
.el-dropdown-menu {
  background: #0688b3!important;
  border: 1px solid #00deff!important;
}
.el-dropdown-menu__item--divided:before {
  content: '';
  height: 1px !important;
  background-color: #00deff!important;
}
.el-dropdown-menu__item--divided {
  border-top: 1px solid transparent!important;
}
.el-popper[x-placement^=bottom] .popper__arrow::after {
  top: 1px;
  margin-left: -6px;
  border-top-width: 0;
  border-bottom-color: #0688b3!important;
}
.el-popper[x-placement^=bottom] .popper__arrow {
  border-bottom-color: #00deff!important;
}
.el-dropdown-menu__item {
  color: #fff!important;
}
.el-dropdown-menu__item:hover {
  background-color: transparent!important;
  color: #00deff!important;
}

.el-dropdown-menu__item--divided:before {
  content: "";
  height: 1px !important;
  background-color: #00deff !important;
  width: 96%;
  margin: 0 auto;
}


.bodyMain {
  margin: 0 auto;
  width: 1845px;
}

.mainWrap {
  flex-wrap: wrap;
  @include fj();

  .sideItem {
    position: relative;
    margin-bottom: 20px;
    width: 910px;
    border: 1px solid #448fe4;
    background: radial-gradient(circle, rgba(18, 69, 113, 0) 0, rgba(18, 69, 113, 1) 160%);

    .markLineArea {
      position: absolute;
      top: 70px;
      right: 25px;
      display: flex;
      align-items: center;

      .colorLine {
        display: inline-block;
        margin-right: 12px;
        width: 30px;
        border-bottom: 1px dashed #ff5975;
      }
    }

    .peopleTip {
      position: absolute;
      top: 65px;
      left: 15px;
      display: flex;
      color: #00edff;

      &::before {
        content: '!';
        display: block;
        margin-right: 10px;
        width: 23px;
        height: 23px;
        border-radius: 50%;
        text-align: center;
        line-height: 23px;
        background-color: #00edff;
        color: #000;
      }
    }
  }

  .width600 {
    width: 600px;
  }

  .width740 {
    width: 740px;
  }

  .width800 {
    width: 800px;
  }

  .width910 {
    width: 910px;
  }

  .width1020 {
    width: 1020px;
  }

  .width1080 {
    width: 1080px;
  }

  .width1340 {
    width: 1340px;
  }

  .width1845 {
    width: 1845px;
  }

  .sideItemChart {
    height: 425px;
    min-height: 425px;
  }
}

.twoWrap {
  margin-bottom: 15px;
  flex-wrap: wrap;
  @include fj();
  width: 100%;
  border: 1px solid #448fe4;
}


.tableWrap,
.tableWrap1 {
  padding: 5px;
  @include sc(20px, #fff);

  li {
    display: flex;
    font-size: 20px;
    text-align: center;

    p {
      flex: 1;
      line-height: 45px;

      &.yellow {
        color: #ffba00;
      }
    }

    &:nth-child(odd) {
      background: #0b274a;
    }
  }
}

.tableWrap1 {
  margin-top: 10px;
  border: 2px solid #007e7d;
}

.imgModalBody {
  position: relative;
  @include fj(center);

  #echartsWrap1 {
    @include wh(95%, 415px);
  }

  .colorArea {
    margin: 10px 0 0 5px;

    p {
      margin-bottom: 5px;

      .txt {
        display: block;
        margin-bottom: 5px;
        white-space: nowrap;
        color: #fff;
      }
    }
  }
}

.fileLoadingModal {
  position: fixed;
  text-align: center;
  @include wh(100%, 100%);
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.8);

  .progressWrap {
    width: 50%;
    @include center()
  }

  .downTxt {

    display: block;
    margin-top: 10px;
  }
}

// 表格边框样式
.tableWrap,
.tableWrap2 {
  margin: 0 auto;
  width: 96%;

  .el-table--enable-row-hover .el-table__body tr:hover>td {
    background-color: #212e3e !important;
  }

  .el-table--border,
  .el-table--group {
    background: transparent !important;
  }

  .el-table th {
    background: #043f85 !important;
    color: #fff;
  }

  .el-table tr:nth-child(odd) {
    background: #061c38;
    color: #00d7e9;
  }

  .el-table tr:nth-child(even) {
    background: #0b274a;
    color: #00d7e9;
  }

  .el-table td,
  .el-table th {
    padding: 8px 0;
    line-height: 30px;
  }

  .el-table td,
  .el-table th.is-leaf {
    border-bottom: none;
  }

  .el-table--border::after,
  .el-table--group::after,
  .el-table::before {
    height: 0;
  }

  .el-table,
  .el-table__expanded-cell {
    background-color: transparent;
  }

  .el-table .cell {
    line-height: 30px;
  }
}
.tableWrap2 {
  .el-table td,
  .el-table th.is-leaf {
    border-bottom: 1px solid #EBEEF5!important;
  }
  .el-table--border {
    border-right: 1px solid #EBEEF5 !important;
  }
}

// elementui样式修改
.formBox {
  .el-form-item__label {
    color: #01cfff;
  }
  .el-input-group__append,
  .el-input-group__prepend {
    padding: 0;
  }
}